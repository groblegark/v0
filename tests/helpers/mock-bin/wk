#!/usr/bin/env bash
# Mock wk command for testing
# Behavior controlled by environment variables:
#   MOCK_WK_LIST_OUTPUT - output for wk list

case "$1" in
    "list")
        if [ -n "$MOCK_WK_LIST_OUTPUT" ]; then
            echo "$MOCK_WK_LIST_OUTPUT"
        fi
        exit 0
        ;;
    "init")
        exit 0
        ;;
    *)
        exit 0
        ;;
esac
